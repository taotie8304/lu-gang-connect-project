// 已使用 json5 进行解析，会自动去掉注释，无需手动去除
{
  "feConfigs": {
    "systemTitle": "鲁港通跨境AI智能平台", // 系统标题 - 品牌定制
    "show_git": false, // 隐藏GitHub链接
    "show_appStore": false, // 隐藏应用商店
    "show_templateMarket": false, // 隐藏模板市场
    "show_pluginMarket": false, // 隐藏插件市场
    "show_promotion": false, // 隐藏推广功能
    "docUrl": "", // 文档链接（暂无）
    "concatMd": "技术支持：鲁港通科技", // 联系方式
    "lafEnv": "https://laf.dev", // laf环境
    "mcpServerProxyEndpoint": "", // mcp server 代理地址
    
    // 用户注册和登录配置
    "register_method": ["email", "phone"], // 注册方式：email-邮箱, phone-手机号
    "login_method": ["email", "phone"], // 登录方式：email-邮箱, phone-手机号
    "find_password_method": ["email", "phone"], // 找回密码方式
    
    // 鲁港通用户界面配置
    "defaultChatAppId": "", // 默认聊天应用 ID，留空则使用第一个可用应用
    "enableUserChatOnly": true, // 启用纯聊天模式，普通用户登录后直接进入聊天界面
    "adminPath": "/admin" // 管理后台路径
  },
  "systemEnv": {
    "datasetParseMaxProcess": 10, // 知识库文件解析最大线程数量
    "vectorMaxProcess": 10, // 向量处理线程数量
    "qaMaxProcess": 10, // 问答拆分线程数量
    "vlmMaxProcess": 10, // 图片理解模型最大处理进程
    "tokenWorkers": 30, // Token 计算线程保持数，会持续占用内存，不能设置太大。
    "hnswEfSearch": 100, // 向量搜索参数，仅对 PG 和 OB 生效。越大，搜索越精确，但是速度越慢。设置为100，有99%+精度。
    "hnswMaxScanTuples": 100000, // 向量搜索最大扫描数据量，仅对 PG生效。
    "customPdfParse": {
      "url": "", // 自定义 PDF 解析服务地址
      "key": "", // 自定义 PDF 解析服务密钥
      "doc2xKey": "", // doc2x 服务密钥
      "price": 0 // PDF 解析服务价格
    }
  }
}
